{
  "name": "todo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "prettier": "prettier --write ."
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "bracketSameLine": true,
    "printWidth": 80,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.1.3",
    "@angular/common": "20.1.3",
    "@angular/compiler": "20.1.3",
    "@angular/core": "20.1.3",
    "@angular/forms": "20.1.3",
    "@angular/platform-browser": "20.1.3",
    "@angular/router": "20.1.3",
    "@ionic/angular": "8.6.5",
    "ionicons": "8.0.13",
    "pocketbase": "0.26.2",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.1.3",
    "@angular/cli": "20.1.3",
    "@angular/compiler-cli": "20.1.3",
    "@ionic/angular-toolkit": "12.2.0",
    "prettier": "3.6.2",
    "typescript": "5.8.3"
  }
}
